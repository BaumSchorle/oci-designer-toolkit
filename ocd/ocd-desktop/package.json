{
  "name": "ocd-desktop",
  "productName": "ocd",
  "version": "0.1.0",
  "description": "OKIT OCD Native Desktop Application",
  "author": {
    "name": "Andrew Hopkinson",
    "email": "andrew.hopkinson@oracle.com",
    "url": "https://github.com/oracle/oci-designer-toolkit"
  },
  "bugs": "https://github.com/oracle/oci-designer-toolkit/issues",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.oracle.okit-ocd",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "build/assets/icon.icns",
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "icon": "build/assets/icon.ico"
    },
    "linux": {
      "icon": "build/assets/icon.png"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.32",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "ocd-react": "file:../ocd-react",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder build -c.extraMetadata.main=build/electron.js",
    "electron-start": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^24.3.1",
    "electron-builder": "^23.6.0"
  },
  "keywords": [
    "ocd",
    "okit",
    "desktop"
  ],
  "license": "UPL-1.0"
}
